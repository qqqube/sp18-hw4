<% @cats.each do |t| %>
	<strong>Kitty Name: <%= t.name %></strong><br>

<% end %>


<% @users.each do |t| %>
	<strong>Username: <%= t.name %></strong><br>
	<strong>Email: <%= t.email %> </strong><br>
	<strong>Age: <%= t.age %> </strong><br>

<% end %>

<% @todos.each do |t| %>
	<strong>Tasks: <%= t.tasks %></strong><br>
	<strong>Finished: <%= t.finished %> </strong><br>

<% end %>

